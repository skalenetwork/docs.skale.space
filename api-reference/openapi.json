  "paths": {
    "/": {
      "post": {
        "summary": "JSON-RPC Endpoint",
        "description": "All JSON-RPC methods are sent as POST requests to this endpoint. The method name is specified in the request body.",
        "operationId": "jsonRpc",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/JsonRpcRequest"
              },
              "examples": {
                "eth_blockNumber": {
                  "summary": "Get latest block number",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_blockNumber",
                    "params": [],
                    "id": 1
                  }
                },
                "eth_getBalance": {
                  "summary": "Get account balance",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getBalance",
                    "params": ["0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb", "latest"],
                    "id": 1
                  }
                },
                "eth_getBlockByNumber": {
                  "summary": "Get block by number",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getBlockByNumber",
                    "params": ["latest", true],
                    "id": 1
                  }
                },
                "eth_getBlockByHash": {
                  "summary": "Get block by hash",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getBlockByHash",
                    "params": ["0x...", true],
                    "id": 1
                  }
                },
                "eth_call": {
                  "summary": "Execute a call",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_call",
                    "params": [{"to": "0x...", "data": "0x..."}, "latest"],
                    "id": 1
                  }
                },
                "eth_sendRawTransaction": {
                  "summary": "Send raw transaction",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_sendRawTransaction",
                    "params": ["0x..."],
                    "id": 1
                  }
                },
                "eth_getTransactionReceipt": {
                  "summary": "Get transaction receipt",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getTransactionReceipt",
                    "params": ["0x..."],
                    "id": 1
                  }
                },
                "eth_getTransactionByHash": {
                  "summary": "Get transaction by hash",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getTransactionByHash",
                    "params": ["0x..."],
                    "id": 1
                  }
                },
                "eth_getTransactionCount": {
                  "summary": "Get transaction count (nonce)",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getTransactionCount",
                    "params": ["0x...", "latest"],
                    "id": 1
                  }
                },
                "eth_estimateGas": {
                  "summary": "Estimate gas for transaction",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_estimateGas",
                    "params": [{"to": "0x...", "data": "0x..."}],
                    "id": 1
                  }
                },
                "eth_getCode": {
                  "summary": "Get contract code",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getCode",
                    "params": ["0x...", "latest"],
                    "id": 1
                  }
                },
                "eth_getStorageAt": {
                  "summary": "Get storage at position",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getStorageAt",
                    "params": ["0x...", "0x0", "latest"],
                    "id": 1
                  }
                },
                "eth_gasPrice": {
                  "summary": "Get current gas price",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_gasPrice",
                    "params": [],
                    "id": 1
                  }
                },
                "eth_getLogs": {
                  "summary": "Get event logs",
                  "value": {
                    "jsonrpc": "2.0",
                    "method": "eth_getLogs",
                    "params": [{"fromBlock": "latest", "toBlock": "latest", "address": "0x..."}],
                    "id": 1
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "JSON-RPC response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JsonRpcResponse"
                },
                "examples": {
                  "success": {
                    "summary": "Successful response",
                    "value": {
                      "jsonrpc": "2.0",
                      "id": 1,
                      "result": "0x1234"
                    }
                  },
                  "error": {
                    "summary": "Error response",
                    "value": {
                      "jsonrpc": "2.0",
                      "id": 1,
                      "error": {
                        "code": -32602,
                        "message": "Invalid params"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "JsonRpcRequest": {
        "type": "object",
        "required": ["jsonrpc", "method", "id"],
        "properties": {
          "jsonrpc": {
            "type": "string",
            "enum": ["2.0"],
            "description": "JSON-RPC version"
          },
          "method": {
            "type": "string",
            "description": "JSON-RPC method name. SKALE supports all standard Ethereum JSON-RPC methods.",
            "examples": [
              "eth_blockNumber",
              "eth_getBalance",
              "eth_getBlockByNumber",
              "eth_getBlockByHash",
              "eth_call",
              "eth_sendRawTransaction",
              "eth_getTransactionReceipt",
              "eth_getTransactionByHash",
              "eth_getTransactionCount",
              "eth_estimateGas",
              "eth_getCode",
              "eth_getStorageAt",
              "eth_gasPrice",
              "eth_getLogs",
              "eth_accounts",
              "eth_coinbase",
              "eth_mining",
              "eth_hashrate",
              "eth_getWork",
              "eth_submitWork",
              "eth_syncing",
              "net_version",
              "net_listening",
              "net_peerCount",
              "web3_clientVersion",
              "web3_sha3"
            ]
          },
          "params": {
            "type": "array",
            "description": "Method parameters (varies by method)",
            "items": {}
          },
          "id": {
            "oneOf": [
              { "type": "integer" },
              { "type": "string" }
            ],
            "description": "Request ID for matching responses"
          }
        }
      },
      "JsonRpcResponse": {
        "type": "object",
        "required": ["jsonrpc", "id"],
        "properties": {
          "jsonrpc": {
            "type": "string",
            "enum": ["2.0"],
            "description": "JSON-RPC version"
          },
          "id": {
            "oneOf": [
              { "type": "integer" },
              { "type": "string" }
            ],
            "description": "Request ID matching the request"
          },
          "result": {
            "description": "Method result (varies by method)"
          },
          "error": {
            "$ref": "#/components/schemas/JsonRpcError"
          }
        }
      },
      "JsonRpcError": {
        "type": "object",
        "required": ["code", "message"],
        "properties": {
          "code": {
            "type": "integer",
            "description": "Error code",
            "examples": [-32700, -32600, -32601, -32602, -32603, -32000, -32001, -32002, -32003, -32004, -32005]
          },
          "message": {
            "type": "string",
            "description": "Error message"
          },
          "data": {
            "description": "Additional error data"
          }
        }
      }
    }
  },
  "x-mint": {
    "title": "JSON-RPC API",
    "description": "Interactive JSON-RPC API playground for SKALE Chains. SKALE Chains implement the full Ethereum JSON-RPC API specification, providing compatibility with all standard Ethereum methods.",
    "apiSections": [
