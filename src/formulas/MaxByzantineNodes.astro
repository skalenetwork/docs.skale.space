<!-- Uses MathML -->
<!-- Link to MathML: https://developer.mozilla.org/en-US/docs/Web/MathML/Element/mo -->
<!-- Link to Symbols: https://www.tutorialspoint.com/mathml/mathml_algebra.htm -->
<!-- h = N - t = N - FLOOR[N-1/3] = FLOOR[2N-1/3] -->
<math class="override-math">
    <mrow displaystyle="true" class="override-row">
        <mi>h</mi>
        <mo>=</mo>
        <mi>N</mi>
        <mo>-</mo>
        <mi>t</mi>
        <mo>=</mo>
        <mi>N</mi>
        <mo>-</mo>
        <mo style="margin-top: 0; height: 5px;">&lfloor;</mo>
        <mfrac>
            <mrow>
                <mi>N</mi>
                <mo>-</mo>
                <mn>1</mn>
            </mrow>
            <mn>3</mn>
        </mfrac>
        <mo>&rfloor;</mo>
        <mo>=</mo>
        <mo>&lfloor;</mo>
        <mfrac>
            <mrow>
                <mn>2</mn>
                <mi>N</mi>
                <mo>-</mo>
                <mn>1</mn>
            </mrow>
            <mn>3</mn>
        </mfrac>
        <mo>&rfloor;</mo>
    </mrow>
</math>

